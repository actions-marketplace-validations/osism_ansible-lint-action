{
   "extends": [
      "github>osism/renovate-config"
   ],
   "regexManagers":[
      {
         "fileMatch":[
            "^README.md$"
         ],
         "matchStrings":[
            "(?<depName>ansible) version: (?<currentValue>.*?)"
         ],
         "datasourceTemplate":"pypi"
      },
      {
         "fileMatch":[
            "^README.md$"
         ],
         "matchStrings":[
            "(?<depName>ansible-lint) version: (?<currentValue>.*?)"
         ],
         "datasourceTemplate":"pypi"
      }
   ]
}
